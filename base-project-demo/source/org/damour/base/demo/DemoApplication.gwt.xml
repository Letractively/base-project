<module>
    
	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>
	<inherits name='com.google.gwt.xml.XML'/>
	<inherits name='com.google.gwt.i18n.I18N' />
	<inherits name='com.google.gwt.http.HTTP' />
	<inherits name='org.damour.base.BaseApplication' />

	<!-- 
    <inherits name="com.google.gwt.user.UserAgent"/>  
    <set-property name="user.agent" value="gecko1_8" />
 	-->

	<!-- supported locales (beyond english defaults) -->
	<extend-property name="locale" values="en"/>
	
	<!-- Specify the app entry point class.                   -->
	<entry-point class='org.damour.base.demo.client.DemoApplication'/>
	
	<servlet path="/servlet/org.damour.base.demo.server.DemoServiceImpl" class="org.damour.base.demo.server.DemoServiceImpl" />
</module>
